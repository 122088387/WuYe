<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_pay_money_store_address"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/White"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/shipping_address_default_img"
            android:layout_width="@dimen/d18"
            android:layout_height="@dimen/d18"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/d10"
            android:src="@drawable/select" />

        <RelativeLayout
            android:id="@+id/rl_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/d25"
            android:padding="@dimen/d12">


            <TextView
                android:id="@+id/shipping_address_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="王晓赛 先生"
                android:textColor="@color/red_fd4420"
                android:textSize="@dimen/TextSize_15" />

            <TextView
                android:id="@+id/shipping_address_phoneNum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/d10"
                android:layout_toRightOf="@id/shipping_address_name"
                android:text="15645545215"
                android:textColor="@color/red_fd4420"
                android:textSize="@dimen/TextSize_15" />

            <TextView
                android:id="@+id/shipping_address_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/shipping_address_name"
                android:layout_marginTop="@dimen/d6"
                android:text="金源商务广场A座606室"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/TextSize_5" />
        </RelativeLayout>

    </RelativeLayout>


    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/itemLineSize"
        android:layout_below="@id/rl_1"
        android:layout_marginLeft="@dimen/d10"
        android:layout_marginRight="@dimen/d10"
        android:background="@color/color_e8e8e8" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/view"
        android:padding="@dimen/d10">

        <RelativeLayout
            android:id="@+id/rl_11"
            android:layout_width="60dp"
            android:layout_alignParentRight="true"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/shipping_address_delete_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="删除"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/d14" />

            <ImageView
                android:id="@+id/shipping_address_delete"
                android:layout_width="@dimen/d25"
                android:layout_height="@dimen/d25"
                android:layout_marginRight="@dimen/d2"
                android:layout_toLeftOf="@id/shipping_address_delete_tv"
                android:padding="@dimen/d2"
                android:src="@drawable/address_delect" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_22"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@id/rl_11">

            <TextView
                android:id="@+id/shipping_address_editor_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:text="编辑"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/d14" />

            <ImageView
                android:id="@+id/shipping_address_editor"
                android:layout_width="@dimen/d25"
                android:layout_height="@dimen/d25"
                android:layout_marginRight="@dimen/d2"
                android:layout_toLeftOf="@id/shipping_address_editor_tv"
                android:padding="@dimen/d2"
                android:src="@drawable/address_editor" />
        </RelativeLayout>


    </RelativeLayout>

</LinearLayout>
